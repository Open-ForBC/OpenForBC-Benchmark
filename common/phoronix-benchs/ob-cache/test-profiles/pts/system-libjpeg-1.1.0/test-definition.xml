<?xml version="1.0"?>
<!--Phoronix Test Suite v4.4.0m1 (Forsand)-->
<PhoronixTestSuite>
  <TestInformation>
    <Title>System JPEG Library Decode</Title>
    <Description>This test measures the time to decode a jpeg file using the system's libjpeg.</Description>
    <ResultScale>ms</ResultScale>
    <Proportion>LIB</Proportion>
    <TimesToRun>10</TimesToRun>
  </TestInformation>
  <TestProfile>
    <Version>1.1.0</Version>
    <SupportedPlatforms>Linux</SupportedPlatforms>
    <SoftwareType>Utility</SoftwareType>
    <TestType>Processor</TestType>
    <Maintainer>Jim Kukunas</Maintainer>
  </TestProfile>
  <TestSettings>
    <Option>
      <DisplayName>DCT method</DisplayName>
      <Identifier>dct-method</Identifier>
      <Menu>
        <Entry>
          <Name>Integer</Name>
          <Value>int</Value>
        </Entry>
        <Entry>
          <Name>Fast Integer</Name>
          <Value>fast</Value>
        </Entry>
        <Entry>
          <Name>Float</Name>
          <Value>float</Value>
        </Entry>
      </Menu>
    </Option>
  </TestSettings>
</PhoronixTestSuite>
