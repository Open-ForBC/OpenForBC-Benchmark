{
  "name": "MNIST realtime benchmark",
  "description": "Training and inference with MNIST dataset",
  "default_preset": "training",
  "test_command": [
    "pip install flake8",
    "flake8 --exclude .venv"
  ],
  "test_preset": "test",
  "setup_command": "./setup.sh",
  "run_command": "./MNIST_realtime.py",
  "stats": {
    "total_time": {
      "regex": "total_time: (\\d+(?:\\.\\d+))"
    },
    "avg_time_per_sample": {
      "regex": "avg_time_per_sample: (\\d+(?:\\.\\d+))"
    }
  },
  "virtualenv": true
}
