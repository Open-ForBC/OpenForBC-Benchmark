{
  "name": "TCGA benchmark topic modeling",
  "description": "A benchmark on TCGA Breast cancer",
  "default_preset": "inference",
  "run_command": {
    "command": "./TCGA_topicmodeling_benchmark.py"
  },
  "cleanup_command": "setup.sh --clean",
  "test_command": [
    "pip install flake8",
    "flake8 --exclude .venv"
  ],
  "test_preset": "test",
  "setup_command": "./setup.sh",
  "stats": {
    "total_time": {
      "regex": "total_time: (\\d+(?:\\.\\d+))"
    },
    "avg_time_per_sample": {
      "regex": "avg_time_per_sample: (\\d+(?:\\.\\d+))"
    }
  },
  "virtualenv": true
}
