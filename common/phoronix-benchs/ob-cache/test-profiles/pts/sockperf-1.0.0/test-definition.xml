<?xml version="1.0"?>
<!--Phoronix Test Suite v8.0.0m4-->
<PhoronixTestSuite>
  <TestInformation>
    <Title>Sockperf</Title>
    <AppVersion>3.4</AppVersion>
    <Description>This is a network socket API performance benchmark.</Description>
    <ResultScale>MB/s</ResultScale>
    <Proportion>HIB</Proportion>
    <TimesToRun>5</TimesToRun>
  </TestInformation>
  <TestProfile>
    <Version>1.0.0</Version>
    <SupportedPlatforms>Linux, MacOSX, BSD</SupportedPlatforms>
    <SoftwareType>Benchmark</SoftwareType>
    <TestType>Network</TestType>
    <License>Free</License>
    <Status>Verified</Status>
    <ExternalDependencies>build-utilities, perl</ExternalDependencies>
    <EnvironmentSize>125</EnvironmentSize>
    <ProjectURL>https://github.com/Mellanox/sockperf</ProjectURL>
    <Maintainer>Michael Larabel</Maintainer>
  </TestProfile>
  <TestSettings>
    <Option>
      <DisplayName>Test</DisplayName>
      <Identifier>run-test</Identifier>
      <ArgumentPrefix></ArgumentPrefix>
      <ArgumentPostfix></ArgumentPostfix>
      <DefaultEntry>0</DefaultEntry>
      <Menu>
        <Entry>
          <Name>Latency Under Load</Name>
          <Value>under-load</Value>
          <Message></Message>
        </Entry>
        <Entry>
          <Name>Latency Ping Pong</Name>
          <Value>ping-pong</Value>
          <Message></Message>
        </Entry>
        <Entry>
          <Name>Throughput</Name>
          <Value>throughput</Value>
          <Message></Message>
        </Entry>
      </Menu>
    </Option>
  </TestSettings>
</PhoronixTestSuite>
