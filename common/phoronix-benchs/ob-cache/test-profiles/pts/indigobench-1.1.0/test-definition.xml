<?xml version="1.0"?>
<!--Phoronix Test Suite v10.0.1-->
<PhoronixTestSuite>
  <TestInformation>
    <Title>IndigoBench</Title>
    <AppVersion>4.4</AppVersion>
    <Description>This is a test of Indigo Renderer's IndigoBench benchmark.</Description>
    <ResultScale>M samples/s</ResultScale>
    <Proportion>HIB</Proportion>
    <TimesToRun>3</TimesToRun>
  </TestInformation>
  <TestProfile>
    <Version>1.1.0</Version>
    <SupportedPlatforms>Linux, Windows, MacOSX</SupportedPlatforms>
    <SoftwareType>Utility</SoftwareType>
    <TestType>System</TestType>
    <License>Non-Free</License>
    <Status>Verified</Status>
    <SupportedArchitectures>x86_64</SupportedArchitectures>
    <ExternalDependencies>build-utilities</ExternalDependencies>
    <RequiresDisplay>TRUE</RequiresDisplay>
    <EnvironmentSize>265</EnvironmentSize>
    <ProjectURL>http://www.indigorenderer.com/indigobench</ProjectURL>
    <Maintainer>Michael Larabel</Maintainer>
  </TestProfile>
  <TestSettings>
    <Default>
      <Arguments>--silent </Arguments>
    </Default>
    <Option>
      <DisplayName>Acceleration</DisplayName>
      <Identifier>accel</Identifier>
      <Menu>
        <Entry>
          <Name>CPU</Name>
          <Value>--cpuonly</Value>
        </Entry>
        <Entry>
          <Name>OpenCL GPU</Name>
          <Value>--gpuonly</Value>
        </Entry>
      </Menu>
    </Option>
    <Option>
      <DisplayName>Scene</DisplayName>
      <Identifier>scene</Identifier>
      <ArgumentPrefix>--scenes </ArgumentPrefix>
      <Menu>
        <Entry>
          <Name>Supercar</Name>
          <Value>supercar</Value>
        </Entry>
        <Entry>
          <Name>Bedroom</Name>
          <Value>bedroom</Value>
        </Entry>
      </Menu>
    </Option>
  </TestSettings>
</PhoronixTestSuite>
