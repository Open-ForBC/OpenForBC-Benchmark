<?xml version="1.0"?>
<!--Phoronix Test Suite v9.4.0m2-->
<PhoronixTestSuite>
  <TestInformation>
    <Title>pmbench</Title>
    <Description>Pmbench is a Linux paging and virtual memory benchmark. This test profile will report the average page latency of the system.</Description>
    <ResultScale>us - Average Page Latency</ResultScale>
    <Proportion>LIB</Proportion>
    <TimesToRun>3</TimesToRun>
  </TestInformation>
  <TestProfile>
    <Version>1.0.2</Version>
    <SupportedPlatforms>Linux</SupportedPlatforms>
    <SoftwareType>Utility</SoftwareType>
    <TestType>Memory</TestType>
    <License>Free</License>
    <Status>Verified</Status>
    <ExternalDependencies>build-utilities, uuid, libxml2</ExternalDependencies>
    <EnvironmentSize>3</EnvironmentSize>
    <ProjectURL>https://bitbucket.org/jisooy/pmbench/</ProjectURL>
    <Maintainer>Michael Larabel</Maintainer>
  </TestProfile>
  <TestSettings>
    <Option>
      <DisplayName>Concurrent Worker Threads</DisplayName>
      <Identifier>cpu-threads</Identifier>
      <ArgumentPrefix>-j </ArgumentPrefix>
      <Menu>
        <Entry>
          <Name>1</Name>
          <Value>1</Value>
        </Entry>
        <Entry>
          <Name>2</Name>
          <Value>2</Value>
        </Entry>
        <Entry>
          <Name>4</Name>
          <Value>4</Value>
        </Entry>
        <Entry>
          <Name>8</Name>
          <Value>8</Value>
        </Entry>
      </Menu>
    </Option>
    <Option>
      <DisplayName>Read-Write Ratio</DisplayName>
      <Identifier>ratio</Identifier>
      <ArgumentPrefix>-r </ArgumentPrefix>
      <Menu>
        <Entry>
          <Name>100% Writes</Name>
          <Value>0</Value>
        </Entry>
        <Entry>
          <Name>50%</Name>
          <Value>50</Value>
        </Entry>
        <Entry>
          <Name>80% Reads 20% Writes</Name>
          <Value>80</Value>
        </Entry>
        <Entry>
          <Name>100% Reads</Name>
          <Value>100</Value>
        </Entry>
      </Menu>
    </Option>
  </TestSettings>
</PhoronixTestSuite>
